#!/bin/bash
GALILEOHOME=/home/rodrigo/galileo2018
PYTHON=/usr/local/anaconda2/bin/python
VMD=/usr/local/bin/vmd
VINA=/usr/bin/vina
AUTODOCK4=/usr/bin/autodock4
AUTOGRID4=/usr/bin/autogrid4

#!/bin/bash

if [ "$1" == "rec2pdbqt" ]
then
    CMD=$GALILEOHOME/bin/rec2pdbqt.sh

elif [ "$1" == "box" ]
then
    CMD=$GALILEOHOME/bin/box.sh

elif [ "$1" == "vina" ]
then
    CMD=$GALILEOHOME/bin/run.vina.sh

elif [ "$1" == "prepare_ad4" ]
then
    CMD=$GALILEOHOME/bin/prepare.ad4.sh

elif [ "$1" == "ad4" ]
then
    CMD=$GALILEOHOME/bin/run.ad4.sh
fi



shift 
eval $CMD $@

